@-webkit-keyframes FadeInAndMoveUpAnimate { 
  0% { opacity: 0; transform: translateY(5px); }
  15% { opacity: 1; transform: translateY(5px); }
  25% { opacity: 1; transform: translateY(5px); }
  50% { opacity: 1; transform: translateY(5px); }
  75% { opacity: 1; transform: translateY(5px); }
  85% { opacity: 1; transform: translateY(-5px); }
  95% { opacity: 1; transform: translateY(-15px); }
  100% { opacity: 1; transform: translateY(-33px); }
}

